<Window x:Class="ListSLG.window.subWindow.GeneralSubWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ListSLG.window.subWindow"
        mc:Ignorable="d"
        
        Title="GeneralSubWindow" Height="350" Width="550">
    <Grid>
        <Label x:Name="GeneralNameLabel" Content="XXXX" HorizontalAlignment="Left" VerticalAlignment="Top" Height="24" Grid.Column="0"  FontSize="9" Margin="40,19,0,0"/>
        <Grid Margin="40,43,40,175">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="40"/>
                <ColumnDefinition Width="40"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Label Content="攻撃" HorizontalAlignment="Center" VerticalAlignment="Center" Height="24"  Grid.Row="0" Grid.Column="0"  FontSize="9"/>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" Margin="5,3,5,3" Grid.Row="0" Grid.Column="1" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
                <TextBlock x:Name="AttackNumTextBlock" HorizontalAlignment="Right" Margin="0,0,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Center" FontSize="9"/>
            </Border>
            <TextBlock x:Name="AttackFromNumTextBlock" HorizontalAlignment="Center" Margin="0,0,0,0" Grid.Row="0" Grid.Column="2" TextWrapping="Wrap" Text="0" VerticalAlignment="Center" FontSize="9"/>
            <Label Content="防御" HorizontalAlignment="Center" VerticalAlignment="Center" Height="24" Grid.Row="1" Grid.Column="0"  FontSize="9"/>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" Margin="5,3,5,3" Grid.Row="1" Grid.Column="1" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
                <TextBlock x:Name="DefenseNumTextBlock" HorizontalAlignment="Right" Margin="0,0,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Center" FontSize="9"/>
            </Border>
            <TextBlock x:Name="DefenseFromNumTextBlock" HorizontalAlignment="Center" Margin="0,0,0,0" Grid.Row="1" Grid.Column="2" TextWrapping="Wrap" Text="0" VerticalAlignment="Center" FontSize="9"/>
            <Label Content="機動" HorizontalAlignment="Center" VerticalAlignment="Center" Height="24"  Grid.Row="2" Grid.Column="0"  FontSize="9"/>
            <Border BorderBrush="Black" BorderThickness="1,1,0.5,0.5" Margin="5,3,5,3" Grid.Row="2" Grid.Column="1" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
                <TextBlock x:Name="ManeuverNumTextBlock" HorizontalAlignment="Right" Margin="0,0,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Center" FontSize="9"/>
            </Border>
            <TextBlock x:Name="ManeuverFromNumTextBlock" HorizontalAlignment="Center" Margin="0,0,0,0" Grid.Row="2" Grid.Column="2" TextWrapping="Wrap" Text="0" VerticalAlignment="Center" FontSize="9"/>
        </Grid>
        <Grid Margin="40,186,40,22">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="1" Grid.RowSpan="4" >
                <TextBlock Name="ped1bTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="1" Grid.RowSpan="4" >
                <TextBlock Name="ped1pTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="1" Grid.RowSpan="2" >
                <TextBlock Name="ped2bbTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="1" Grid.RowSpan="2" >
                <TextBlock Name="ped2bpTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="1" Grid.Row="4" Grid.ColumnSpan="1" Grid.RowSpan="2" >
                <TextBlock Name="ped2pbTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="1" Grid.Row="6" Grid.ColumnSpan="1" Grid.RowSpan="2" >
                <TextBlock Name="ped2ppTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2" Grid.Row="0" Grid.ColumnSpan="1" Grid.RowSpan="1" >
                <TextBlock Name="ped3bbbTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2" Grid.Row="1" Grid.ColumnSpan="1" Grid.RowSpan="1" >
                <TextBlock Name="ped3bbpTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2" Grid.Row="2" Grid.ColumnSpan="1" Grid.RowSpan="1" >
                <TextBlock Name="ped3bpbTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2" Grid.Row="3" Grid.ColumnSpan="1" Grid.RowSpan="1" >
                <TextBlock Name="ped3bppTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2" Grid.Row="4" Grid.ColumnSpan="1" Grid.RowSpan="1" >
                <TextBlock Name="ped3pbbTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2" Grid.Row="5" Grid.ColumnSpan="1" Grid.RowSpan="1" >
                <TextBlock Name="ped3pbpTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2" Grid.Row="6" Grid.ColumnSpan="1" Grid.RowSpan="1" >
                <TextBlock Name="ped3ppbTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
            <Border BorderBrush="Black" BorderThickness="1" Grid.Column="2" Grid.Row="7" Grid.ColumnSpan="1" Grid.RowSpan="1" >
                <TextBlock Name="ped3pppTextBlock" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="aaaaaa" VerticalAlignment="Center"/>
            </Border>
        </Grid>
    </Grid>
</Window>
